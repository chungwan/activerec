<h1>List of Images</h1>

<table table class="table table-bordered">
  <tr>
    <th>Image Id</th>
    <th>Image Location</th>
  </tr>

<% @images.each do |image| %>
  <tr>
    <td><%= image.id %></td>
    <td><%= image.location %></td>
  </tr>
<% end %>
</table>



